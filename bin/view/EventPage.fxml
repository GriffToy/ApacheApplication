<AnchorPane prefHeight="325.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.EventController">
   <children>
      <VBox spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="16.0" AnchorPane.topAnchor="0.0">
         <children>
            <GridPane hgap="10.0" vgap="10.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="163.0" minWidth="10.0" prefWidth="147.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="191.0" minWidth="10.0" prefWidth="18.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="191.0" minWidth="10.0" prefWidth="152.0" />
              </columnConstraints>
              <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="Select Event:" GridPane.rowIndex="1" />
                  <ComboBox fx:id="eventsComboBox" prefHeight="25.0" prefWidth="182.0" GridPane.columnIndex="2" GridPane.rowIndex="1" />
                  <Label text="Event Details" GridPane.rowIndex="2" />
                  <Label text="Date and Time" GridPane.rowIndex="4" />
                  <Label text="Registration Cut-off Date" GridPane.rowIndex="5" />
                  <Label text="Sponsors" GridPane.rowIndex="6" />
                  <Label text="Judges" GridPane.rowIndex="7" />
                  <Label fx:id="eventDetails" text="None" GridPane.columnIndex="2" GridPane.rowIndex="2" />
                  <Label fx:id="dateAndTime" text="None" GridPane.columnIndex="2" GridPane.rowIndex="4" />
                  <Label fx:id="cutOff" text="None" GridPane.columnIndex="2" GridPane.rowIndex="5" />
                  <Label fx:id="sponsors" text="None" GridPane.columnIndex="2" GridPane.rowIndex="6" />
                  <Label fx:id="judges" text="None" GridPane.columnIndex="2" GridPane.rowIndex="7" />
                  <Label text="Location" GridPane.rowIndex="3" />
                  <Label fx:id="eventLocation" text="None" GridPane.columnIndex="2" GridPane.rowIndex="3" />
                  <Label text="Event List">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Label>
               </children>
            </GridPane>
            <HBox spacing="10.0">
               <children>
                  <Region maxHeight="-Infinity" minHeight="-Infinity" prefHeight="25.0" HBox.hgrow="ALWAYS" />
                  <Button fx:id="leftButton" mnemonicParsing="false" onAction="#leftButtonClicked" prefWidth="90.0" text=" " visible="false" />
                  <Button fx:id="rightButton" defaultButton="true" mnemonicParsing="false" onAction="#rightButtonClicked" prefWidth="90.0" text="Back" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
