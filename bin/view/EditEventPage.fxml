                  <Button fx:id="deleteEventButton" mnemonicParsing="false" onAction="#deleteEventButtonClicked" prefHeight="31.0" prefWidth="111.0" text="Delete Event" />
                  <Button fx:id="saveEventButton" defaultButton="true" mnemonicParsing="false" onAction="#saveEventButtonClicked" prefHeight="31.0" prefWidth="111.0" text="Save Event" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
