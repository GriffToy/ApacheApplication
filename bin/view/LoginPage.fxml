<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.LoginPageController">
   <children>
      <AnchorPane>
         <children>
            <VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="323.0" prefWidth="550.0" spacing="10.0" BorderPane.alignment="TOP_LEFT">
               <children>
                  <GridPane hgap="10.0" prefHeight="110.0" prefWidth="520.0" vgap="10.0">
                     <children>
                        <TextField fx:id="userNameField" maxWidth="342.0" prefHeight="31.0" prefWidth="302.0" promptText="Username" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                        <Label text="Log In" textAlignment="CENTER" GridPane.columnIndex="1">
                           <font>
                              <Font name="System Bold" size="18.0" />
                           </font>
                        </Label>
                        <Label alignment="CENTER" prefHeight="21.0" prefWidth="132.0" text="Username:" GridPane.rowIndex="1" />
                        <Label alignment="CENTER" prefHeight="21.0" prefWidth="131.0" text="Password:" GridPane.rowIndex="2" />
                        <PasswordField fx:id="passwordField" maxWidth="341.0" prefHeight="31.0" prefWidth="341.0" promptText="Password" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                     </children>
                     <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="205.0" minWidth="10.0" prefWidth="124.0" />
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="383.0" minWidth="10.0" prefWidth="368.0" />
                     </columnConstraints>
                     <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                     </rowConstraints>
                  </GridPane>
                  <Label fx:id="invalidLoginLabel" text=" " textFill="RED" />
                  <Label prefHeight="40.0" prefWidth="482.0" text="Planning to attend an event but no account yet? Click Register to create. " />
                  <HBox alignment="TOP_RIGHT" spacing="10.0">
                     <children>
                        <Region HBox.hgrow="ALWAYS" />
                        <Button fx:id="showEventsButton" mnemonicParsing="false" onAction="#showEventsButtonClicked" prefHeight="31.0" prefWidth="125.0" text="Show Events" />
                        <Button fx:id="registerButton" mnemonicParsing="false" onAction="#registerButtonClicked" prefHeight="31.0" prefWidth="111.0" text="Register" />
                        <Button fx:id="loginButton" defaultButton="true" mnemonicParsing="false" onAction="#loginButtonClicked" prefHeight="31.0" prefWidth="119.0" text="Login" />
                     </children>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </HBox>
               </children>
               <BorderPane.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </BorderPane.margin>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </VBox>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
