<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.AttendeeController">
   <children>
      <VBox spacing="10.0">
         <children>
            <GridPane>
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="Event" GridPane.rowIndex="2" />
                  <Label text="Category" GridPane.rowIndex="3" />
                  <Label text="Fibers" GridPane.rowIndex="4" />
                  <Label text="Self-dyed Yarn" GridPane.rowIndex="5" />
                  <Label text="Handspun Yarn" GridPane.rowIndex="6" />
                  <Label text="Other Details" GridPane.rowIndex="7" />
                  <Label fx:id="eventLabel" text="None" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <Label fx:id="categoryLabel" text="None" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <Label fx:id="fibersLabel" text="None" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                  <Label fx:id="selfDyedLabel" text="None" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                  <Label fx:id="handspunLabel" text="None" GridPane.columnIndex="1" GridPane.rowIndex="6" />
                  <Label fx:id="otherDetailsLabel" text="None" GridPane.columnIndex="1" GridPane.rowIndex="7" />
                  <Label text="View Entry:" />
                  <ComboBox fx:id="selectEntryComboBox" prefHeight="25.0" prefWidth="114.0" promptText="None" GridPane.columnIndex="1" />
                  <Label text="Entry Details" GridPane.rowIndex="1">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font></Label>
               </children>
            </GridPane>
            <HBox spacing="10.0">
               <children>
                  <Region HBox.hgrow="ALWAYS" />
                  <Button fx:id="newEntryButton" defaultButton="true" mnemonicParsing="false" onAction="#newEntryButtonClicked" text="New Entry" />
                  <Button fx:id="logoutButton" mnemonicParsing="false" onAction="#logoutButtonClicked" text="Logout" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
